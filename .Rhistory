coord_polar()
# 그래프 출력
print(p)
# 원 그래프를 그리는 함수
circle_function <- function(theta) {
return(c(cos(theta), sin(theta)))
}
# 원 그래프를 그리는 ggplot 객체 생성
p <- ggplot(data.frame(theta = c(0, 2 * pi)), aes(x = circle_function(theta)[1], y = circle_function(theta)[2])) +
stat_function(fun = circle_function, geom = "path") +
coord_equal() +  # x축과 y축의 비율을 동일하게 맞춤
theme_minimal()
# 그래프 출력
print(p)
